{
  "name": "server",
  "version": "1.0.0",
  "description": "Nestjs w/ Apollo server, Prisma like ORM-layer",
  "license": "MIT",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "nodemon -e ts,graphql -x ts-node src/main.ts",
