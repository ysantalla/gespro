    "apollo-server-core": "^2.2.6",
    "apollo-server-express": "^2.2.6",
    "dotenv": "^6.1.0",
    "graphql": "^14.0.0",
    "graphql-import": "^0.7.1",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.0",
    "graphql-type-json": "^0.2.1",
    "jsonwebtoken": "^8.3.0",
    "mkdirp": "^0.5.1",
    "prisma-client-lib": "~1.29.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.1",
    "shortid": "^2.2.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^5.6.2",
    "@nestjs/testing": "^5.4.0",
    "@types/graphql": "^14.0.0",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.9.4",
    "@types/shortid": "^0.0.29",
    "nodemon": "^1.18.8",
    "prisma": "^1.29.0",
    "ts-node": "^7.0.1",
